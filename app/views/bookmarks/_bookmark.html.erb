
<% link = bookmark.thumbnail_url.blank? ? bookmark.url  : "<img src='#{bookmark.thumbnail_url}'/>".html_safe %>
<%= link_to link, bookmark, class: "thumbs" %><br />


<%= render partial: 'likes/like', locals: {bookmark: bookmark } %>


<% if user_bookmark = current_user.user_bookmark_for(bookmark) %>
  <%= link_to user_bookmark, method: :delete, class: 'btn btn-danger' do %>
  <i class="glyphicon glyphicon-remove"></i>&nbsp;Delete 
  <% end %>
<% end %>

